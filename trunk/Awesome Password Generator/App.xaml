<Application x:Class="Awesome_Password_Generator.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <JumpList.JumpList>
        <JumpList>
            <JumpTask Title="Wi-Fi WPA passphrase"
                Description="Generate Wi-Fi WPA passphrase"
                Arguments="--quickgen:wpa"
                IconResourceIndex="0" />
            <JumpTask Title="Password"
                Description="Generate a single password"
                Arguments="--quickgen:password"
                IconResourceIndex="0" />
        </JumpList>
    </JumpList.JumpList>
    
    <Application.Resources>
         
    </Application.Resources>
</Application>
